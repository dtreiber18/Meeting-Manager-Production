# Meeting Manager Environment Variables

# Database Configuration
DB_USERNAME=meetingmanager
DB_PASSWORD=password
DB_URL=jdbc:mysql://localhost:3306/meeting_manager
MONGODB_URI=mongodb://localhost:27017/meeting_manager

# JWT Configuration
JWT_SECRET=changeme-to-secure-256-bit-key
JWT_EXPIRATION=86400000

# Microsoft Graph API Configuration
MICROSOFT_CLIENT_ID=your-microsoft-client-id
MICROSOFT_CLIENT_SECRET=your-microsoft-client-secret
MICROSOFT_TENANT_ID=your-microsoft-tenant-id
MICROSOFT_REDIRECT_URI=http://localhost:4200/auth/callback

# Frontend Configuration
FRONTEND_URL=http://localhost:4200

# Azure OpenAI (Future)
AZURE_OPENAI_ENDPOINT=your-azure-openai-endpoint
AZURE_OPENAI_API_KEY=your-azure-openai-api-key

# Azure Text Analytics (Future)
AZURE_TEXT_ANALYTICS_ENDPOINT=your-text-analytics-endpoint
AZURE_TEXT_ANALYTICS_API_KEY=your-text-analytics-api-key
